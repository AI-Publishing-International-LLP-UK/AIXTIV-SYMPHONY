# Cloud Run Environment Variables for Integration Gateway
NODE_ENV=production
PORT=8080
PROJECT_ID=api-for-warp-drive
SERVICE_ACCOUNT=integration-gateway@api-for-warp-drive.iam.gserviceaccount.com
REGION=us-west1

# Authentication
CLOUDFLARE_AUTH_ENABLED=true
SALLYPORT_AUTH_ENABLED=true

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_ENABLED=true
MAX_REQUESTS_PER_MINUTE=100

# Health Check
HEALTH_CHECK_ENABLED=true
