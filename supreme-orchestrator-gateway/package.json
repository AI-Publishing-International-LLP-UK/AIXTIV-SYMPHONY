{
  "name": "supreme-orchestrator-gateway",
  "version": "1.0.0",
  "description": "Hardened Enterprise OAuth2 Gateway for Dr. Claude sRIX Supreme Orchestrator",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/",
    "security-audit": "npm audit --audit-level=moderate",
    "setup-secrets": "./scripts/setup-secrets.sh",
    "docker:build": "docker build -t supreme-orchestrator-gateway .",
    "docker:run": "docker run -p 8443:8443 supreme-orchestrator-gateway",
    "gcp:deploy": "./scripts/deploy-to-cloud-run.sh",
    "health-check": "curl -f http://localhost:8443/health || exit 1",
    "logs": "gcloud logging read 'resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"supreme-orchestrator-gateway\"' --limit=50 --format=json"
  },
  "keywords": [
    "oauth2",
    "supreme-orchestrator",
    "dr-claude-srix",
    "universal-auth-orchestrator",
    "enterprise-security",
    "cascading-authorization",
    "google-cloud",
    "mocorix2"
  ],
  "author": "AI Publishing International LLP",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "winston": "^3.10.0",
    "crypto": "^1.0.1",
    "@google-cloud/secret-manager": "^5.0.1",
    "@google-cloud/logging": "^10.5.0",
    "@google-cloud/monitoring": "^4.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "bcrypt": "^5.1.0",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "eslint": "^8.45.0",
    "@types/node": "^20.4.5"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AI-Publishing-International-LLP-UK/supreme-orchestrator-gateway.git"
  },
  "config": {
    "port": 8443,
    "environment": "production",
    "gcp_project": "api-for-warp-drive",
    "gcp_region": "us-central1"
  },
  "security": {
    "audit_level": "high",
    "vulnerability_scan": true,
    "dependency_check": true
  }
}