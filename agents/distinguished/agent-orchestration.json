        {
          "name": "squadron_formation",
          "agent": "pilot_coordinator",
          "inputs": ["rics_configuration"],
          "outputs": ["squadron_configuration"],
          "next": "copilot_assignment"
        },
        {
          "name": "copilot_assignment",
          "agent": "copilot_manager",
