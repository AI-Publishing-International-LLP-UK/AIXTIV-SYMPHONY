          "agent": "companion_agent",
          "inputs": ["community_needs", "user_profiles"],
          "outputs": ["companion_assignments"],
          "next": "crx_setup"
        },
        {
          "name": "crx_setup",
          "agent": "crx_agent",
          "inputs": ["service_requirements", "user_profiles"],
          "outputs": ["crx_configuration"],
          "next": "deployment"
        },
        {
          "name": "deployment",
          "agent": "flight_management",
          "inputs": ["rics_configuration", "squadron_configuration", "copilot_assignments", "companion_assignments", "crx_configuration"],
          "outputs": ["deployment_report"],
          "next": "monitoring"
        },
        {
          "name": "monitoring",
          "agent": "dream_commander",
          "inputs": ["deployment_report", "user_feedback"],
          "outputs": ["optimization_recommendations"],
          "next": "organization_analysis"
        }
      ]
    },
